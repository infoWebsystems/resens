      }
    },
    "text_rich_text_bngeLP": {
      "type": "text--rich-text",
      "blocks": {
        "richtext_4x3hfA": {
          "type": "richtext",
          "settings": {
            "text": "<p>\"Sustainable marketing should be, first and foremost, internal marketing. Only thing we can be sure of is changing ourselves!\" <br\/><strong>- Yulia Polyakova and Olga Guzhyk. <\/strong><\/p><p>\"My goal is to make a beautiful product without impoverishing the future of my daughters.\" <strong>- Yulia Gagarina.<\/strong><\/p>",
            "text_size": 18
          }
        }
      },
      "block_order": [
        "richtext_4x3hfA"
      ],
      "settings": {
        "background_color": "#f4f4f4",
        "colors": "white",
