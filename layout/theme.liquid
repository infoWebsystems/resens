  {% comment %}
  Cart Drawer, also used by 'Quick View', and 'Search'
  {% endcomment %}
  {% comment %} {% render 'right-drawer' %} {% endcomment %}
  {% section 'right-drawer' %}

  <div id="DrawerOverlay" class="drawer-overlay"></div>

  <div id="sendResumeForm" class="form-modal hidden">
    {% assign form_fields = shop.metaobjects.vacancies_form['vacancy-form'] %}
    <div class="form-modal-head">
      <h3 class="form-modal-title">
        {{ form_fields.heading }}
      </h3>
      <div class="form-modal-close">
        <svg width="35" height="34" viewBox="0 0 35 34" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M25.8996 8.6001L9.09961 25.4001" stroke="#262428" stroke-width="2.1" stroke-linecap="round" stroke-linejoin="round"/>
          <path d="M9.09961 8.6001L25.8996 25.4001" stroke="#262428" stroke-width="2.1" stroke-linecap="round" stroke-linejoin="round"/>
        </svg>
      </div>
    </div>

    <div class="form-modal-form-wrapper">
      {% form 'contact' %}
        {{ form.errors | default_errors }}
      
        <div class="form-modal-fields">
          <div class="form-modal-field name">
            <label for="name">{{ form_fields.name_label }}</label>
            <input type="text" name="contact[first_name]" id="name" placeholder="{{ form_fields.name_placeholder }}" required/>
          </div>
        
          <div class="form-modal-field city">
            <label for="city">{{ form_fields.country_label }}</label>
            <input type="tel" name="contact[city]" id="city" placeholder="{{ form_fields.country_placeholder }}" required/>
          </div>

          <div class="form-modal-field cooperation-form">
            <label for="cooperation-form">{{ form_fields.wished_form_collaboration_label }}</label>

            <select id="cooperation-form" name="contact[cooperation-form]" required>
              <option value="" disabled selected>{{ form_fields.wished_form_collaboration_option_placeholder }}</option>
              
              {% assign topic_string = form_fields.wished_form_collaboration_options | remove: '[' | remove: ']' | remove: '"' %}
              {% assign topic_options = topic_string | split: ',' %}
              
              {% for option in topic_options %}
                  <option value="{{ option }}">{{ option }}</option>
              {% endfor %}
            </select>
          </div>

          <div class="form-modal-field unit">
            <label for="unit">{{ form_fields.unit_label }}</label>

            <select id="unit" name="contact[unit]" required>
              <option value="" disabled selected>{{ form_fields.unit_option_placeholder }}</option>

              {% assign topic_string = form_fields.unit_options | remove: '[' | remove: ']' | remove: '"' %}
              {% assign topic_options = topic_string | split: ',' %}
              
              {% for option in topic_options %}
                  <option value="{{ option }}">{{ option }}</option>
              {% endfor %}
            </select>
          </div>

          <div class="form-modal-field phone">
            <label for="phone">{{ form_fields.phone_label }}</label>
            <input type="tel" name="contact[phone]" id="phone" placeholder="{{ form_fields.phone_placeholder }}" required/>
          </div>
        
          <div class="form-modal-field email">
            <label for="email">{{ form_fields.email_label }}</label>
            <input type="email" name="contact[email]" id="email" placeholder="{{ form_fields.email_placeholder }}" required/>
          </div>
        </div>

        <div class="submit form-modal-submit" data-is-submitted-successfully="{{ form.posted_successfully? }}">
          <input class="btn" type="submit" value="{{ form_fields.submit_button_text }}" />
        </div>
      {% endform %}
    </div>
  </div>

  <div id="partnershipForm" class="form-modal hidden">
    {% assign form_fields = shop.metaobjects.partnership_form['partnership-form'] %}
    <div class="form-modal-head">
      <h3 class="form-modal-title">
        {{ form_fields.heading }}
      </h3>
      <div class="form-modal-close">
        <svg width="35" height="34" viewBox="0 0 35 34" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M25.8996 8.6001L9.09961 25.4001" stroke="#262428" stroke-width="2.1" stroke-linecap="round" stroke-linejoin="round"/>
          <path d="M9.09961 8.6001L25.8996 25.4001" stroke="#262428" stroke-width="2.1" stroke-linecap="round" stroke-linejoin="round"/>
        </svg>
      </div>
    </div>

    <div class="form-modal-form-wrapper">
      {% form 'contact' %}
        <div class="form-modal-fields">
          <div class="form-modal-field subject">
            <label for="subject">{{ form_fields.topic_label }}</label>

            <select id="subject" name="contact[subject]" required>
              <option value="" disabled selected>{{ form_fields.topic_label_option_placeholder }}</option>
              
              {% assign topic_string = form_fields.topic_options | remove: '[' | remove: ']' | remove: '"' %}
              {% assign topic_options = topic_string | split: ',' %}
              
              {% for option in topic_options %}
                  <option value="{{ option }}">{{ option }}</option>
              {% endfor %}
            </select>
          </div>
        
          <div class="form-modal-field name">
            <label for="name">{{ form_fields.name_label }}</label>
            <input type="text" name="contact[first_name]" id="name" placeholder="{{ form_fields.name_placeholder }}" required/>
          </div>
        
          <div class="form-modal-field country">
            <label for="country">{{ form_fields.country_label }}</label>
            <input type="tel" name="contact[country]" id="country" placeholder="{{ form_fields.country_placeholder }}" required/>
          </div>

          <div class="form-modal-field city">
            <label for="city">{{ form_fields.city_label }}</label>
            <input type="tel" name="contact[city]" id="city" placeholder="{{ form_fields.city_placeholder }}" required/>
          </div>

          <div class="form-modal-field phone">
            <label for="phone">{{ form_fields.phone_label }}</label>
            <input type="tel" name="contact[phone]" id="phone" placeholder="{{ form_fields.phone_placeholder }}" required/>
          </div>
        
          <div class="form-modal-field email">
            <label for="email">{{ form_fields.email_label }}</label>
            <input type="email" name="contact[email]" id="email" placeholder="{{ form_fields.email_placeholder }}" required/>
          </div>
        </div>

        <div class="submit form-modal-submit" data-is-submitted-successfully="{{ form.posted_successfully? }}">
          <input class="btn" type="submit" value="{{ form_fields.submit_button_text }}" />
        </div>
      {% endform %}
    </div>
  </div>

  <div id="thankYouModal" class="modal-thank-you hidden">
    {% assign form_fields = shop.metaobjects.thank_you_modal['thank-you-modal'] %}

      <div class="modal-thank-you-close-icon modal-thank-you-close">
        <svg width="35" height="34" viewBox="0 0 35 34" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M25.8996 8.6001L9.09961 25.4001" stroke="#262428" stroke-width="2.1" stroke-linecap="round" stroke-linejoin="round"/>
          <path d="M9.09961 8.6001L25.8996 25.4001" stroke="#262428" stroke-width="2.1" stroke-linecap="round" stroke-linejoin="round"/>
        </svg>
      </div>

      <h3 class="modal-thank-you-title">
        {{ form_fields.heading }}
      </h3>

      <p class="modal-thank-you-message">
        {{ form_fields.thank_you_message }}
      </p>

      <button class="btn modal-thank-you-button modal-thank-you-close">{{ form_fields.continue_button_text }}</button>
  </div>

  <div class="form-overlay hidden"></div>

  <div id="PageContainer" data-editor-open="{{ request.design_mode }}" data-cart-action="{{ settings.ajax_cart_method }}" data-language-url="{{ localization.language.root_url }}">
    <div class="no-js disclaimer">
      <p>This store requires javascript to be enabled for some features to work correctly.</p>
    </div>


        {% sections 'header-group' %}


    {% if settings.enable_breadcrumbs %}
      {% unless template == 'index' or template == 'cart' or template == 'search' %}
        {% comment %} {% unless template.name == 'collection' or template == 'list-collections' or template contains 'contact' or template.name == 'product' %} {% endcomment %}
        {% unless template.name == 'collection' or template == 'list-collections' or template.name == 'product' %}
          <div id="breadcrumbs" class="wrapper-padded">
            {% render 'breadcrumb' %}
          </div>
        {% endunless %}
      {% endunless %}
    {% endif %}

    {% if template == 'index' %}
      <div class="flow-homepage">
    {% elsif template == 'cart' or template contains 'customers/' %}
      <div class="main-content page-main--wrapper">
    {% else %}
      <div class="wrapper main-content page-main--wrapper">
    {% endif %}

        {{ content_for_layout }}
      </div>
  </div>

  <div class="footer-wrapper">
      {% sections 'footer-group' %}
    </div>

  <script defer type="module" src="{{ 'theme.min.js' | asset_url }}"></script>
  <script defer src="{{ 'SafeLoadScripts.js' | asset_url }}"></script>
  <script defer nomodule src="{{ 'theme.legacy.min.js' | asset_url }}"></script>

  {% if settings.popup--enable %}
    {% render 'popup' %}
  {% endif %}

  {% if settings.enable_verification_popup %}
    {% render 'verification-popup' %}
  {% endif %}

  {% render 'back-to-top-button' %}

  <script type="application/json" id="wetheme-global">
    {
      {% comment %}Translations that will be used in javascript section handling{% endcomment %}
      "translations": {
        "addToCart": {{ 'products.product.add_to_cart' | t | json }},
        "added": {{ 'products.product.added' | t | json }},
        "soldOut": {{ 'products.product.sold_out' | t | json }},
        "unavailable": {{ 'products.product.unavailable' | t | json }},
        "no_results_html": {{ 'general.search.no_results_html' | t | json }},
        "results_for_html": {{ 'general.search.results_for_html' | t | json }},
        "products": {{ 'general.search.products' | t | json }},
        "pages": {{ 'general.search.pages' | t | json }},
        "collections": {{ 'general.search.collections' | t | json }},
        "articles": {{ 'general.search.articles' | t | json }},
        "suggestions": {{ 'general.search.suggestions' | t | json }},
        "quick_shop": {{ 'products.product.quick_shop' | t | json }},
        "view_options": {{ 'products.product.view_options' | t | json }},
        "read_more_html": {{ 'products.product.read_more_html' | t | json }},
        "read_less_html": {{ 'products.product.read_less_html' | t | json }},
        "youve_viewed": {{ 'collections.paginate.youve_viewed' | t | json }},
        "load_more": {{ 'collections.paginate.load_more' | t | json }},
        "productAdded": {{ 'products.product.added' | t | json }},
        "loading": {{ 'collections.paginate.loading' | t | json }},
        "giftwrap_label": {{ 'products.general.giftwrap_label' | t | json }},
        "yes": {{ 'products.general.yes' | t | json }},
        "days": {{ 'general.time.days' | t | json }},
        "hours": {{ 'general.time.hours' | t | json }},
        "minutes": {{ 'general.time.minutes' | t | json }},
        "seconds": {{ 'general.time.seconds' | t | json }}
      },
      "moneyFormat": {{ shop.money_format | json }},
      "giftwrapProduct": {{ settings.giftwrap_product_selector | json }}
    }
  </script>

  {% if request.design_mode %}<script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>{% endif %}
    
  {% render 'currencies' %}
</body>
</html>
